library(tidyverse)
library(magrittr)
library(ggplot2)
library(patchwork)
gss <- read_csv('../datasets/GSS_religion.csv')
head(gss)
summary(gss$prottemp)
summary(gss$cathtemp)
factor(gss$prottemp) %>% levels
factor(gss$cathtemp) %>% levels
# Count the number of responses with valid responses across prottemp and cathtemp
sum(!is.na(gss$prottemp) & (gss$prottemp >= 0) & (gss$prottemp <=100) & !is.na(gss$cathtemp) & (gss$cathtemp >= 0) & (gss$cathtemp <=100))
diff_prottemp_cathtemp <- gss$prottemp - gss$cathtemp
summary(diff_prottemp_cathtemp)
hist(diff_prottemp_cathtemp)
